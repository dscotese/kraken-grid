{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": true,
    "incremental": true,
    "lib": ["ES2023", "DOM"],
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "outDir": ".dist",
    "paths": {
      "*": ["*"]
    },
    "downlevelIteration": true,
    "rootDir": ".",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2020",
    "typeRoots": ["./node_modules/@types", "./types"]
  },
    "include": [
    "*.ts",
    "test/**/*.ts",
  ],
  "exclude": [
    "node_modules",
    ".dist",
    ".dist/**/*",
    "**/*.js",
    "**/*.js.map",
    "TFC*",
    "TFCGem",
    "TFCGem/**/*",
    "JestTesting"
  ],
  "files": ["global.d.ts", "unconverted.d.ts"]
}