{
  "name": "kraken-grid",
  "version": "1.3.2",
  "description": "Bot repeatedly buys & sells on kraken from a conditional close order.",
  "main": "./.dist/init.js",
  "bin": {
    "kraken-grid": "./.dist/init.js",
    "gemini-grid": "./.dist/ginit.js"
  },
  "type": "module",
  "scripts": {
    "testK": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/kraken.test --debug --verbose --detectOpenHandles --runInBand",
    "testG": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/gem.test --debug --verbose --detectOpenHandles --runInBand",
    "watch": "tsc --watch",
    "start": "node .dist/init.js",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dscotese/kraken-grid.git"
  },
  "keywords": [
    "kraken",
    "bot",
    "grid",
    "crypto"
  ],
  "author": "Dave Scotese",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dscotese/kraken-grid/issues"
  },
  "homepage": "https://github.com/dscotese/kraken-grid#readme",
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "body-parser": "^1.20.2",
    "concat-stream": "^2.0.0",
    "cryptex": "^1.0.1",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-session": "^1.17.3",
    "glob": "^10.1.0",
    "got": "^14.4.5",
    "http-server": "^14.1.1",
    "kraka-djs": "^1.0.3",
    "nvm": "^0.0.4",
    "prompt-sync": "^4.2.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@types/cacheable-request": "^6.0.3",
    "@types/http-cache-semantics": "^4.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "ava": "^4.3.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "directories": {
    "test": "test"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./types.ts",
        "default": [
          "./.dist/init.js",
          "./init.js"
        ]
      }
    },
    "./*": {
      "import": {
        "types": "./*.d.ts",
        "default": [
          "./.dist/*.js",
          "./*.js"
        ]
      }
    }
  }
}
